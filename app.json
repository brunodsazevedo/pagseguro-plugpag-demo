{
  "expo": {
    "name": "pagseguro-plugpag-demo",
    "slug": "pagseguro-plugpag-demo",
    "scheme": "pagseguro-plugpag-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.brunodsazevedo.pagseguroplugpagdemo"
    },
    "plugins": [
      "react-native-pagseguro-plugpag",
      "expo-router"
    ]
  }
}
